{
  "name": "atlauncher-next",
  "version": "0.0.1",
  "description": "ATLauncher NEXT is the next iteration of ATLauncher.",
  "main": "app/main.js",
  "scripts": {
    "build": "npm run build:main && npm run build:app",
    "build:app": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build:main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "dist": "npm run build && rimraf dist/ && cross-env NODE_ENV=production node -r babel-register package.js",
    "hot-server": "node -r babel-register server.js",
    "lint": "eslint app *.js",
    "start": "cross-env NODE_ENV=production electron ./app/main.js",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./main",
    "test": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js app/**/*.spec.js",
    "test-e2e": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "test-watch": "npm test -- --watch"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATLauncher/ATLauncher-NEXT.git"
  },
  "author": {
    "name": "RyanTheAllmighty",
    "email": "ryantheallmighty@atlauncher.com",
    "url": "https://github.com/RyanTheAllmighty"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ATLauncher/ATLauncher-NEXT/issues"
  },
  "homepage": "https://github.com/ATLauncher/ATLauncher-NEXT#readme",
  "build": {
    "appId": "com.atlauncher.next",
    "app-category-type": "public.app-category.games",
    "win": {
      "iconUrl": "https://github.com/ATLauncher/ATLauncher-NEXT/blob/initial-code/build/icon.ico?raw=true"
    }
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  },
  "productName": "ATLauncher NEXT",
  "productCopyright": "Copyright 2016 ATLauncher - Licensed under MIT",
  "electronVersion": "1.3.3",
  "devDependencies": {
    "archiver": "^1.0.0",
    "babel-core": "^6.13.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "copyfiles": "^1.0.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "devtron": "^1.2.1",
    "electron": "^1.3.3",
    "electron-debug": "^1.0.1",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^7.3.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-import-resolver-webpack": "^0.5.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "express": "^4.14.0",
    "jsdom": "^9.4.2",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "node-libs-browser": "^1.0.0",
    "react-addons-test-utils": "^15.3.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "source-map-support": "^0.4.1",
    "spectron": "^3.2.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0"
  }
}
